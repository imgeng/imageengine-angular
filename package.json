{
    "name": "@imageengine/angular-workspace",
    "version": "0.0.3",
    "description": "Angular (12) workspace for ImageEngine components",
    "homepage": "https://github.com/imgeng/imageengine-angular",
    "repository": {
        "type": "git",
        "url": "https://github.com/imgeng/imageengine-angular"
    },
    "author": "Micael Nussbaumer",
    "license": "MIT",
    "scripts": {
	"ng": "ng",
	"start": "ng serve",
	"publish": "ng build && npm publish ./dist/ngx-imageengine/ --access public",
